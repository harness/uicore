    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.3",
    "lodash-es": "^4.17.21",
    "marked": "^4.0.12",
    "masonry-layout": "^4.2.2",
    "mini-css-extract-plugin": "^1.6.2",
    "moment": "^2.25.3",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.0",

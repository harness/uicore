    "react-draggable": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.0.4",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.0",
    "style-loader": "^3.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^9.2.3",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "webpack": "^5.41.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0",
    "yup": "^0.32.8",
    "@harness/icons": "1.0.0"
  },
  "resolutions": {
    "@babel/core": "7.14.6",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "postcss": "8.3.5"
  },
  "lint-staged": {
    "*": [
      "scripts/license/stamp.sh"
    ],
    "src/**/*.{js,jsx}": [
      "sh -c 'exit 1'"
    ],
    "*.{ts,tsx},!docs/**/*": [
      "eslint",
      "prettier --check",
      "sh scripts/typecheck-staged.sh"
    ],
    "*.css": [
      "prettier --check",
      "stylelint"
    ]
  },
  "files": [
    "dist/*",
    "tools/*"
  ]
}

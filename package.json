{
  "name": "@harness/uicore",
  "version": "3.5.0",
  "description": "Harness UICore - Legos for building Harness UI applications",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "style": "dist/index.css",
  "types": "dist/index.d.ts",
  "scripts": {
    "release": "standard-version",
    "start": "yarn dep; yarn ui:icons; run-p -c ui:types ui:watch docs:watch",
    "prebuild": "yarn clean",
    "build": "yarn ui:build; yarn types:copy",
    "clean": "rm -rf docs/static dist",
    "dep": "yarn; cd docs; yarn; cd ..",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" && stylelint ./src && prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "jest --no-cache ./src",
    "jest": "jest --no-cache --watch",
    "test:ci": "jest --no-cache ./src --ci  --coverage --reporters=jest-junit --reporters=default --runInBand --silent",
    "fmt": "prettier --write \"./src/**/*.{ts,tsx,css}\"",
    "ui:types": "tsc --emitDeclarationOnly --watch --declarationDir ./docs/static/",
    "ui:watch": "NODE_ENV=development webpack --watch",
    "ui:build": "NODE_ENV=production webpack",
    "docs:watch": "cd docs; yarn watch",
    "setup": "bash scripts/github-package-registry.sh",
    "types:copy": "echo 'Adhoc type copy...'; mv dist/src/* dist/; rm -rf dist/src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "do-puppeteer-storyshots": "./node_modules/.bin/jest --projects=./storyshots-puppeteer",
    "prepare": "husky install",
    "puppeteer-storyshots": "yarn run build-storybook && yarn run do-puppeteer-storyshots"
  },
  "license": "UNLICENSED",
  "repository": "https://github.com/harness/uicore",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.20.0",
    "@blueprintjs/datetime": "3.13.0",
    "@blueprintjs/select": "3.10.0",
    "@harness/use-modal": "1.0.0",
    "anser": "^2.1.0",
    "classnames": "^2.2.6",
    "clipboard-copy": "^3.1.0",
    "lodash-es": "^4.17.15",
    "marked": "^4.0.12",
    "masonry-layout": "^4.2.2",
    "moment": "^2.25.3",
    "mustache": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.0.4",
    "@harness/icons": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@blueprintjs/core": "^3.20.0",
    "@blueprintjs/datetime": "3.13.0",
    "@blueprintjs/select": "3.10.0",
    "@harness/design-system": "1.0.0",
    "@harness/use-modal": "1.0.0",
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-docs": "^6.3.1",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/addon-storyshots": "^6.3.6",
    "@storybook/addon-storyshots-puppeteer": "^6.3.6",
    "@storybook/builder-webpack5": "^6.3.1",
    "@storybook/manager-webpack5": "^6.3.1",
    "@storybook/react": "^6.3.1",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^26.0.19",
    "@types/lodash-es": "^4.17.4",
    "@types/marked": "^4.0.3",
    "@types/masonry-layout": "^4.2.1",
    "@types/mustache": "^4.1.2",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-table": "7.0.13",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "anser": "^2.1.0",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",

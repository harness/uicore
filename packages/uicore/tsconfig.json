{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "alwaysStrict": true,
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "target": "ES6",
    "jsx": "react",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false
  },
  "include": ["./src/**/*"]
}

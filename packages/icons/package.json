  },
  "devDependencies": {
    "@svgr/core": "^6.2.1",
    "@types/react": "^17.0.3",
    "dts-bundle-generator": "^6.4.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "typescript": "^4.1.3",
    "vite": "^2.7.13",
    "vite-plugin-dts": "^0.9.9",
    "@svgr/plugin-svgo": "^6.2.0"
